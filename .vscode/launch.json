{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Current TS File",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/ts-node/dist/_bin.js",
			"args": ["--fast", "${relativeFile}"],
			"cwd": "${workspaceRoot}",
			"protocol": "inspector",
			"env": {
				"DEBUG": "printer,bitcoin.de"
			}
		}
	]
}
